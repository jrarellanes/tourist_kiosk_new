<%- model_class = Preference -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:iduser) %>:</strong></dt>
  <dd><%= @preference.iduser %></dd>
  <dt><strong><%= model_class.human_attribute_name(:idsite) %>:</strong></dt>
  <dd><%= @preference.idsite %></dd>
  <dt><strong><%= model_class.human_attribute_name(:dateRegister) %>:</strong></dt>
  <dd><%= @preference.dateRegister %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              preferences_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_preference_path(@preference), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              preference_path(@preference),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>
