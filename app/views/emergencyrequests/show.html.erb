<%- model_class = Emergencyrequest -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:iduser) %>:</strong></dt>
  <dd><%= @emergencyrequest.iduser %></dd>
  <dt><strong><%= model_class.human_attribute_name(:idcorporation) %>:</strong></dt>
  <dd><%= @emergencyrequest.idcorporation %></dd>
  <dt><strong><%= model_class.human_attribute_name(:dateRequest) %>:</strong></dt>
  <dd><%= @emergencyrequest.dateRequest %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              emergencyrequests_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_emergencyrequest_path(@emergencyrequest), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              emergencyrequest_path(@emergencyrequest),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>
